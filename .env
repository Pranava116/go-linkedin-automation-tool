# LinkedIn Automation Framework - Environment Configuration
# ⚠️ FOR EDUCATIONAL PURPOSES ONLY - DO NOT USE ON REAL ACCOUNTS

# LinkedIn Credentials (use dummy/test account only)
LINKEDIN_EMAIL=your-dummy-email@example.com
LINKEDIN_PASSWORD=your-dummy-password

# Browser Configuration
BROWSER_HEADLESS=false
BROWSER_USER_AGENT="Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Safari/537.36"

# Stealth Configuration
STEALTH_MIN_DELAY=1s
STEALTH_MAX_DELAY=3s
STEALTH_RESPECT_BUSINESS_HOURS=true

# Rate Limiting (very conservative for safety)
RATE_LIMIT_CONNECTIONS_PER_HOUR=5
RATE_LIMIT_MESSAGES_PER_HOUR=3
RATE_LIMIT_SEARCHES_PER_HOUR=10

# Storage
STORAGE_TYPE=sqlite
STORAGE_PATH=./data
STORAGE_DATABASE=linkedin_automation.db

# Logging
LOGGING_LEVEL=debug
LOGGING_FORMAT=text
LOGGING_OUTPUT=stdout
